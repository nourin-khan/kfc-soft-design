<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity" xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions"
	xmlns:local="clr-namespace:KFC_Table_GUI"
	mc:Ignorable="d"
	x:Class="KFC_Table_GUI.UserControlOptionsBar"
	x:Name="UserControl" Height="34" Width="34">
	<UserControl.Resources>
		<Storyboard x:Key="addToCartEnter"/>
		<Storyboard x:Key="addToCartNormal"/>
		<Storyboard x:Key="closeEnter"/>
		<Storyboard x:Key="closeLeave"/>
	</UserControl.Resources>

	<Grid x:Name="LayoutRoot" Margin="0,0,0,0.063">
		<local:UserControlOK x:Name="okControl" Width="Auto" Margin="0,0,0,0.031" MouseLeftButtonUp="okControl_MouseLeftButtonUp" />
	</Grid>
</UserControl>